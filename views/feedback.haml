= categories({})

#feedback
  %h1 Send Us Feedback
  %p We love hearing from people, so please fill in the following and let us know what you think!
  %form{:action => "/email", :method => "post"}
    %label
      Name
      %input{:type => "text", :name => "name", :size => 30}
    %label
      Email
      %input{:type => "text", :name => "email", :size => 30}
    %label
      Subject
      %input{:type => "text", :name => "subject", :size => 50, :value => @subject}
    %label
      Message
      %textarea{:name => "message", :rows => 8, :cols => 55}
    %input{:type => "hidden", :name => "url", :value => @url}
    %input{:type => "submit", :name => "b", :value => "Send Comments"}
